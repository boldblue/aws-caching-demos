{
    "executionRoleArn": "arn:aws:iam::675959897322:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "aws-caching-demo",
            "image": "aws-caching-demos",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}